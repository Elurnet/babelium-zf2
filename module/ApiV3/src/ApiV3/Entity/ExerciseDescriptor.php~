<?php

namespace ApiV3\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ExerciseDescriptor
 *
 * @ORM\Table(name="exercise_descriptor", uniqueConstraints={@ORM\UniqueConstraint(name="code", columns={"situation", "level", "competence", "number"})})
 * @ORM\Entity
 */
class ExerciseDescriptor
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var boolean
     *
     * @ORM\Column(name="situation", type="boolean", nullable=false)
     */
    private $situation = '1';

    /**
     * @var boolean
     *
     * @ORM\Column(name="level", type="boolean", nullable=false)
     */
    private $level = '1';

    /**
     * @var boolean
     *
     * @ORM\Column(name="competence", type="boolean", nullable=false)
     */
    private $competence = '1';

    /**
     * @var integer
     *
     * @ORM\Column(name="number", type="integer", nullable=false)
     */
    private $number = '1';

    /**
     * @var boolean
     *
     * @ORM\Column(name="alte", type="boolean", nullable=false)
     */
    private $alte = '0';

    /**
     * @var \Doctrine\Common\Collections\Collection
     *
     * @ORM\ManyToMany(targetEntity="ApiV3\Entity\Exercise", mappedBy="fkExerciseDescriptor")
     */
    private $fkExercise;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->fkExercise = new \Doctrine\Common\Collections\ArrayCollection();
    }

}
